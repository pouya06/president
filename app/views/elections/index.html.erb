<h1>Listing elections</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Hillary clinton</th>
      <th>Martin omalley</th>
      <th>Bernie sanders</th>
      <th>Jeb bush</th>
      <th>Ben carson</th>
      <th>Chris christie</th>
      <th>Ted cruz</th>
      <th>Carly fiorina</th>
      <th>Mike huckabee</th>
      <th>John kasich</th>
      <th>Rand paul</th>
      <th>Marco rubio</th>
      <th>Rick santorum</th>
      <th>Donald trump</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @elections.each do |election| %>
      <tr>
        <td><%= election.first_name %></td>
        <td><%= election.last_name %></td>
        <td><%= election.hillary_clinton %></td>
        <td><%= election.martin_omalley %></td>
        <td><%= election.bernie_sanders %></td>
        <td><%= election.jeb_bush %></td>
        <td><%= election.ben_carson %></td>
        <td><%= election.chris_christie %></td>
        <td><%= election.ted_cruz %></td>
        <td><%= election.carly_fiorina %></td>
        <td><%= election.mike_huckabee %></td>
        <td><%= election.john_kasich %></td>
        <td><%= election.rand_paul %></td>
        <td><%= election.marco_rubio %></td>
        <td><%= election.rick_santorum %></td>
        <td><%= election.donald_trump %></td>
        <td><%= link_to 'Show', election %></td>
        <td><%= link_to 'Edit', edit_election_path(election) %></td>
        <td><%= link_to 'Destroy', election, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

